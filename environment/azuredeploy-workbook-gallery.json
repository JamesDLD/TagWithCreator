          },
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "resources\n| where \"{unTaggableResources}\" !has type \n| extend compliant = iff(tags.editor == \"\" or tags.lasteditiondate == \"\" or tags.creationdate == \"\" or tags.creator == \"\",\"Not Compliant\", \"Compliant\")\n| summarize count(type) by compliant",
